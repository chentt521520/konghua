<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/bai"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/gdxqit_name"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="管理员"
            android:textColor="@color/select_te"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/gdxqit_tetime"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="12dp"
            android:gravity="center_vertical"
            android:text="2018-08-23 09:17"
            android:textColor="@color/select_te"
            android:textSize="12sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/gdxqit_tetxt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="7dp"
        android:gravity="start"
        android:lineSpacingExtra="7dp"
        android:text="2018-08-23 09:17"
        android:textColor="@color/select_te"
        android:textSize="12sp" />

    <LinearLayout
        android:id="@+id/gdxqit_lintp"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/gdxqit_imtp1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingStart="7dp"
            android:paddingTop="5dp"
            android:paddingRight="7dp"
            android:src="@mipmap/sctpx" />


        <ImageView
            android:id="@+id/gdxqit_imtp2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingRight="7dp"
            android:src="@mipmap/sctpx" />


        <ImageView
            android:id="@+id/gdxqit_imtp3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingRight="7dp"
            android:src="@mipmap/sctpx" />

    </LinearLayout>

    <View
        android:id="@+id/gdxq_view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="5dp"
        android:background="@color/qianhei1" />
</LinearLayout>
